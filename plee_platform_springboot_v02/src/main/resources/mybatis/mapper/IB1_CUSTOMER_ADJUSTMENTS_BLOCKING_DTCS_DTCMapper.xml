<?xml version="1.0" encoding="UTF8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!--namespace绑定一个对应的Mapper接口（Dao）-->
<mapper namespace="com.fawvw.dao.IB1_CUSTOMER_ADJUSTMENTS_BLOCKING_DTCS_DTCMapper">

    <select id="getIB1_CUSTOMER_ADJUSTMENTS_BLOCKING_DTCS_DTC" resultType="com.fawvw.pojo.IB1_CUSTOMER_ADJUSTMENTS_BLOCKING_DTCS_DTCLei">
        SELECT * FROM IB1_CUSTOMER_ADJUSTMENTS_BLOCKING_DTCS_DTC
    <where>
        <if test = "platform_name != null">
            platform_name = #{platform_name}
        </if>
        <if test = "IB1_VERSION != null">
            and IB1_VERSION = #{IB1_VERSION}
        </if>
        <if test = "diagnose_adr != null">
            and diagnose_adr = #{diagnose_adr}
        </if>
    </where>
    </select>

    <delete id="deleteIB1_CUSTOMER_ADJUSTMENTS_BLOCKING_DTCS_DTC" parameterType="int">
        delete from IB1_CUSTOMER_ADJUSTMENTS_BLOCKING_DTCS_DTC
        <where>
            <if test = "platform_name != null">
                platform_name = #{platform_name}
            </if>
            <if test = "IB1_VERSION != null">
                and IB1_VERSION = #{IB1_VERSION}
            </if>
            <if test = "diagnose_adr != null">
                and diagnose_adr = #{diagnose_adr}
            </if>
        </where>
    </delete>


</mapper>